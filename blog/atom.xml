---
layout: feed
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

	<title>Owen Martin's Blog</title>
	<link href="http://owingit.github.io/blog/atom.xml" rel="self"/>
	<link href="http://owingit.github.io/blog"/>
	<updated>{{ site.time | date_to_xmlschema }}</updated>
	<id>http://owingit.github.io/blog</id>
	<author>
		<name>Owen Martin</name>
		<email>martin.owen22@gmail.com</email>
	</author>

	{% for page in site.posts %}
		<entry>
			<title>{{ page.title }}</title>
			<link href="http://owingit.github.io{{ post.url }}"/>
			<updated>{{ page.date | date_to_xmlschema }}</updated>
			<id>http://owingit.github.io/{{ page.id }}</id>
			<content type="html">{{ post.content | xml_escape }}</content>
		</entry>
	{% endfor %}

</feed>
